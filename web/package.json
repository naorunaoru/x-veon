{
  "name": "xtrans-demosaic-web",
  "private": true,
  "type": "module",
  "scripts": {
    "setup": "rustup target add wasm32-unknown-unknown && npm install && npm run build:wasm",
    "build:wasm": "npm run build:wasm:decoder && npm run build:wasm:encoder",
    "build:wasm:decoder": "wasm-pack build ../../rawloader-wasm --target web --release --out-dir \"$PWD/wasm-pkg\"",
    "build:wasm:encoder": "wasm-pack build wasm-encoder --target web --release",
    "dev": "npm run build:wasm && vite",
    "build": "npm run build:wasm && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "onnxruntime-web": "^1.21.0"
  },
  "devDependencies": {
    "vite": "^6.0.0",
    "vite-plugin-wasm": "^3.4.1",
    "wasm-pack": "^0.14.0"
  }
}
