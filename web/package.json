{
  "name": "xtrans-demosaic-web",
  "private": true,
  "type": "module",
  "scripts": {
    "setup": "rustup target add wasm32-unknown-unknown && npm install && npm run build:wasm",
    "build:wasm": "npm run build:wasm:decoder && npm run build:wasm:encoder && npm run build:wasm:demosaic",
    "build:wasm:decoder": "wasm-pack build wasm/rawloader --target web --release",
    "build:wasm:encoder": "wasm-pack build wasm/encoder --target web --release",
    "build:wasm:demosaic": "wasm-pack build wasm/demosaic --target web --release",
    "dev": "npm run build:wasm && vite",
    "build": "npm run build:wasm && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "onnxruntime-web": "^1.21.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@webgpu/types": "^0.1.69",
    "vite": "^6.0.0",
    "vite-plugin-wasm": "^3.4.1",
    "wasm-pack": "^0.14.0"
  }
}
